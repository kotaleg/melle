<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}" class="{{pagetype}}">
<!--<![endif]-->
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>{{ title }}</title>
<base href="{{ base }}" />
{% if description %}
<meta name="description" content="{{ description }}" />
{% endif %}

<meta property="og:site_name" content="{{site_name}}" />
<meta property="og:url" content='{{base}}'/>
<meta property="og:title" content="{{title}}"/>
<meta property="og:image" content="{{og.image}}"/>
<meta property="og:description" content="{{description}}"/>
<meta name="twitter:card" content="summary_large_image"/>
<meta name="twitter:image" content="{{og.image}}"/>

<link rel="preload" href="{{base}}catalog/view/theme/melle/fonts/CenturyGothic-Bold.woff" as="font" type="font/woff" crossorigin>
<link rel="preload" href="{{base}}catalog/view/theme/melle/fonts/PTRoubleSans.otf" as="font" type="font/otf" crossorigin>
<link rel="preload" href="{{base}}catalog/view/theme/melle/fonts/CenturyGothicRegular/CenturyGothicRegular.woff" as="font" type="font/woff" crossorigin>

<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
    integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
<link href="{{base}}catalog/view/theme/melle/stylesheet/bootstrap.css{{cache_postfix}}" rel="stylesheet" type="text/css" />

{% for style in styles %}
<link href="{{ style.href }}{{cache_postfix}}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}

{% for link in links %}
<link href="{{ link.href }}" rel="{{ link.rel }}" />
{% endfor %}

<script src="{{base}}catalog/view/javascript/jquery/jquery-2.2.4.min.js" type="text/javascript"></script>

{% for state in states %}
<script type="text/javascript">window.__{{state.codename}}__ = {{state.json}};</script>
{% endfor %}

{% for script in scripts %}
<script src="{{ script }}{{cache_postfix}}" type="text/javascript"></script>
{% endfor %}

{% for analytic in analytics %}
{{ analytic }}
{% endfor %}


{% if success_data != false %}
<script type="text/javascript">
(window["rrApiOnReady"] = window["rrApiOnReady"] || []).push(function() {
    try {
        rrApi.order({
            transaction: "{{success_data.order_id}}",
            items: {{success_data.order_offers}}
        });
    } catch(e) {}
    {% if success_data.email %}
    try {
        rrApi.setEmail("{{success_data.email}}", { name: "{{success_data.name}}" });
    } catch(e) {}
    {% endif %}
})
</script>
{% endif %}


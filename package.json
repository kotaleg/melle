{
  "name": "melle",
  "version": "1.0.0",
  "description": "Melle",
  "main": "index.js",
  "author": "Ivan St. <brokeyourbike@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "yarn run development --watch",
    "watch-poll": "yarn run --watch --watch-poll",
    "prod": "yarn run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-import1c": "yarn run watch --env.mixfile=webpack.import-1c.mix",
    "prod-import1c": "yarn run production --env.mixfile=webpack.import-1c.mix",
    "watch-offersadmin": "yarn run watch --env.mixfile=webpack.offers.mix",
    "prod-offersadmin": "yarn run production --env.mixfile=webpack.offers.mix",
    "watch-offers": "yarn run watch --env.mixfile=webpack.offers-real.mix",
    "prod-offers": "yarn run production --env.mixfile=webpack.offers-real.mix",
    "watch-discount": "yarn run watch --env.mixfile=webpack.discount.mix",
    "prod-discount": "yarn run production --env.mixfile=webpack.discount.mix",
    "watch-sizelist": "yarn run watch --env.mixfile=webpack.sizelist.mix",
    "prod-sizelist": "yarn run production --env.mixfile=webpack.sizelist.mix",
    "watch-pricelist": "yarn run watch --env.mixfile=webpack.pricelist.mix",
    "prod-pricelist": "yarn run production --env.mixfile=webpack.pricelist.mix",
    "watch-melleb": "yarn run watch --env.mixfile=webpack.melleb.mix",
    "prod-melleb": "yarn run production --env.mixfile=webpack.melleb.mix"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@riophae/vue-treeselect": "^0.0.38",
    "axios": "^0.18.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^5.2.0",
    "laravel-mix": "^2.1.11",
    "lodash": "^4.17.10",
    "pretty-checkbox-vue": "^1.1.9",
    "stickyfilljs": "^2.1.0",
    "v-click-outside": "^2.0.2",
    "v-file-upload": "^3.1.7",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.16",
    "vue-carousel": "^0.18.0",
    "vue-fixed-header": "^3.2.2",
    "vue-good-table": "^2.12.2",
    "vue-js-modal": "^1.3.28",
    "vue-js-toggle-button": "^1.2.3",
    "vue-loading-overlay": "^2.1.0",
    "vue-notification": "^1.3.12",
    "vue-progress": "^0.2.1",
    "vue-rate-it": "^2.1.0",
    "vue-recaptcha": "^1.1.1",
    "vue-router": "^3.0.2",
    "vue-select": "^2.5.1",
    "vue-simple-progress": "^1.1.0",
    "vue-slider-component": "^2.8.6",
    "vue-sticky": "^3.3.4",
    "vue-sticky-directive": "^0.0.9",
    "vue-the-mask": "^0.11.1",
    "vue2-datepicker": "^2.10.0",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.0.1",
    "webpack-bundle-analyzer": "^2.11.2"
  }
}

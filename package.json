{
  "name": "melle",
  "version": "1.1.0",
  "description": "Melle",
  "author": "Ivan St. <brokeyourbike@gmail.com>",
  "private": true,
  "scripts": {
    "development:watch": "mix watch",
    "development:production": "mix --production",
    "watch:scripts": "npm run development:watch -- --mix-config=webpack.scripts.mix",
    "watch:styles": "npm run development:watch -- --mix-config=webpack.styles.mix",
    "watch": "npm run watch:scripts && npm run watch:styles",
    "prod:scripts": "npm run development:production -- --mix-config=webpack.scripts.mix",
    "prod:styles": "npm run development:production -- --mix-config=webpack.styles.mix",
    "prod": "npm run prod:scripts && npm run prod:styles",
    "check-pretty": "prettier -l 'catalog/view/theme/melle/{stylesheet/sass/**/,javascript/src/**/,template/**/}*.{js,json,css,scss,vue}'",
    "pretty": "prettier --write 'catalog/view/theme/melle/{stylesheet/sass/**/,javascript/src/**/,template/**/}*.{js,json,css,scss,vue}'"
  },

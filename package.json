{
  "name": "melle",
  "version": "1.1.0",
  "description": "Melle",
  "author": "Ivan St. <brokeyourbike@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev:watch": "yarn run dev --watch",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch:scripts": "npm run dev:watch -- --env.mixfile=webpack.scripts.mix",
    "prod:scripts": "npm run production -- --env.mixfile=webpack.scripts.mix",
    "watch:styles": "npm run dev:watch -- --env.mixfile=webpack.styles.mix",
    "prod:styles": "npm run production -- --env.mixfile=webpack.styles.mix",
    "watch": "npm run watch:scripts && npm run watch:styles",
    "prod": "npm run prod:scripts && npm run prod:styles",
    "check-pretty": "prettier -l 'catalog/view/{theme/melle/stylesheet/sass/**/,javascript/melle/src/**/,theme/melle/template/**/}*.{js,json,css,scss,vue,twig}'",
    "pretty": "prettier --write 'catalog/view/{theme/melle/stylesheet/sass/**/,javascript/melle/src/**/,theme/melle/template/**/}*.{js,json,css,scss,vue,twig}'"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "^0.0.38",
    "algoliasearch": "^4.2.0",
    "axios": "^0.18.0",
    "laravel-mix": "^5",
    "lodash": "^4.17.10",
    "md5": "^2.2.1",
    "pretty-checkbox-vue": "^1.1.9",
    "stickyfilljs": "^2.1.0",
    "v-click-outside": "^2.0.2",
    "v-file-upload": "^3.1.7",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.16",
    "vue-carousel": "^0.18.0",
    "vue-fixed-header": "^3.2.2",
    "vue-good-table": "^2.12.2",
    "vue-instantsearch": "^2.7.0",
    "vue-js-modal": "^1.3.28",
    "vue-js-toggle-button": "^1.2.3",
    "vue-loading-overlay": "^2.1.0",
    "vue-notification": "^1.3.12",
    "vue-progress": "^0.2.1",
    "vue-rate-it": "^2.1.0",
    "vue-recaptcha": "^1.1.1",
    "vue-router": "^3.0.2",
    "vue-select": "^2.5.1",
    "vue-simple-progress": "^1.1.0",
    "vue-slider-component": "^3.1.2",
    "vue-sticky": "^3.3.4",
    "vue-sticky-directive": "^0.0.9",
    "vue-the-mask": "^0.11.1",
    "vue-zoom-on-hover": "^1.0.5",
    "vue2-datepicker": "^2.10.0",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.0.1",
    "vuex-pathify": "^1.4.5"
  },
  "devDependencies": {
    "babel-plugin-lodash": "^3.3.4",
    "bootstrap": "^4.5.3",
    "cross-env": "^7.0.3",
    "laravel-mix-purgecss": "^5",
    "melody-idom": "^1.1.0",
    "melody-runtime": "^1.1.0",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "prettier-plugin-twig-melody": "^0.4.6",
    "sass": "^1.30.0",
    "sass-loader": "^8",
    "vue-template-compiler": "^2.6.12",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie 11",
    "not ie_mob 11"
  ]
}

{
  "name": "melle",
  "version": "1.0.0",
  "description": "Melle",
  "main": "index.js",
  "author": "Ivan St. <brokeyourbike@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-import1c": "npm run watch -- --env.mixfile=webpack.import-1c.mix",
    "prod-import1c": "npm run production -- --env.mixfile=webpack.import-1c.mix",
    "watch-offersadmin": "npm run watch -- --env.mixfile=webpack.offers.mix",
    "prod-offersadmin": "npm run production -- --env.mixfile=webpack.offers.mix",
    "watch-offers": "npm run watch -- --env.mixfile=webpack.offers-real.mix",
    "prod-offers": "npm run production -- --env.mixfile=webpack.offers-real.mix",
    "watch-discount": "npm run watch -- --env.mixfile=webpack.discount.mix",
    "prod-discount": "npm run production -- --env.mixfile=webpack.discount.mix"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "es6-promise": "^4.2.4",
    "javascript-obfuscator": "^0.18.1",
    "laravel-mix": "^2.1.11",
    "lodash": "^4.17.10",
    "pretty-checkbox-vue": "^1.1.9",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "typicons.font": "^2.0.9",
    "v-click-outside": "^2.0.2",
    "v-file-upload": "^3.1.7",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.16",
    "vue-good-table": "^2.12.2",
    "vue-js-modal": "^1.3.28",
    "vue-js-toggle-button": "^1.2.3",
    "vue-loading-overlay": "^2.1.0",
    "vue-notification": "^1.3.12",
    "vue-progress": "^0.2.1",
    "vue-rate-it": "^2.1.0",
    "vue-recaptcha": "^1.1.1",
    "vue-router": "^3.0.2",
    "vue-select": "^2.5.1",
    "vue-simple-progress": "^1.1.0",
    "vue-slider-component": "^2.8.6",
    "vue-the-mask": "^0.11.1",
    "vuex": "^3.0.1",
    "webpack-obfuscator": "^0.18.0"
  }
}

				<td class="left">{{ entry_tax_class }}</td>
				<td class="left">{{ entry_geo_zone }}</td>
				<td class="right">{{ entry_status }}</td>
				<td class="right">{{ entry_sort_order }}</td>
				<td></td>
			  </tr>
			</thead>
			
			
			{% if modules.1 is defined %}
			{% set module_row = 1 %}
			{% for module  in modules %}
			
			<tbody id="module-row{{ module_row }}" class="table table-striped table-bordered table-hover">
			  <tr class="hidedostavim{{ module_row }}">
			    <td class="left">
				<input type="text" name="shipping_dostavimchekaut[{{ module_row }}][name]" id="dostavimname{{ module_row }}" value="{{ module['name'] }}" size="40" />
				</td>
			    <td class="left">
                {% if module_row > 5 %}				
				<input type="text" name="shipping_dostavimchekaut[{{ module_row }}][cost]" value="{{ module['cost'] }}" size="5" />
				{% endif %}				
				</td>
				<td class="left"><select name="shipping_dostavimchekaut[{{ module_row }}][tax_class_id]">
                  <option value="0">{{ text_none }}</option>
				  {% for tax_class  in tax_classes %}
				  {% if tax_class['tax_class_id'] == module['tax_class_id'] %}
                  <option value="{{ tax_class['tax_class_id'] }}" selected="selected">{{ tax_class['title'] }}</option>
				  {% else %}
                  <option value="{{ tax_class['tax_class_id'] }}">{{ tax_class['title'] }}</option>
				  {% endif %}
				  {% endfor %}
                </select></td>
				<td class="left"><select name="shipping_dostavimchekaut[{{ module_row }}][geo_zone_id]">
                <option value="0">{{ text_all_zones }}</option>
				{% for geo_zone  in geo_zones %}
				{% if geo_zone['geo_zone_id'] == module['geo_zone_id'] %}				
                <option value="{{ geo_zone['geo_zone_id'] }}" selected="selected">{{ geo_zone['name'] }}</option>
                {% else %}
                <option value="{{ geo_zone['geo_zone_id'] }}" selected="selected">{{ geo_zone['name'] }}</option>
                {% endif %}
				{% endfor %}
              </select></td>
				<td class="right"><select name="shipping_dostavimchekaut[{{ module_row }}][status]">
				{% if module['status'] %}	
                <option value="1" selected="selected">{{ text_enabled }}</option>
                <option value="0">{{ text_disabled }}</option>
                {% else %}
                <option value="1">{{ text_enabled }}</option>
                <option value="0" selected="selected">{{ text_disabled }}</option>
                {% endif %}
              </select></td>
				<td class="right"><input type="text" name="shipping_dostavimchekaut[{{ module_row }}][sort_order]" value="{{ module['sort_order'] }}" size="3" /></td>
				
				<td class="left">
				{% if module_row > 5 %}
				<a onclick="$('#module-row{{ module_row }}').remove();" class="btn btn-primary">{{ button_remove }}</a>
				{% endif %}				
				</td>
			  </tr>
			</tbody>
			{% set module_row = module_row + 1 %}
			
			{% endfor %}




{% else %}
			
			{% set module_row = 1 %}
			{% for module  in search_array_module %}
			<tbody id="module-row{{ module_row }}">
			  <tr>
			    <td class="left"><input type="text" name="shipping_dostavimchekaut[{{ module_row }}][name]" value="{{ module['name'] }}" size="40" /></td>
